{
  "applicationId": "js.neutralino.zero",
  "version": "1.0.0",
  "defaultMode": "window",
  "documentRoot": "/www/",
  "url": "/",
  "enableServer": true,
  "enableNativeAPI": true,
  "nativeAllowList": [
    "app.*"
  ],
  "modes": {
    "window": {
      "title": "Mic Level Meter",
      "width": 800,
      "height": 500,
      "maxWidth": 40,
      "maxHeight": 800,
      "borderless": false,
      "resizable": true,
      "enableInspector": true,
      "icon": "/www/icon.png"
    }
  },
  "cli": {
    "binaryName": "mic-level-meter",
    "resourcesPath": "/www/",
    "extensionsPath": "/extensions/",
    "binaryVersion": "5.2.0",
    "clientVersion": "5.2.0"
  }
}
